    "gulp-concat": "^2.6.1",
    "gulp-header": "^1.8.8",
    "gulp-less": "^3.3.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-postcss": "^6.4.0",
    "gulp-rename": "^1.2.2",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "ignore-styles": "^5.0.1",
    "json-loader": "^0.5.4",
    "less": "^2.7.2",
