{
  "name": "b-rc",
  "version": "0.8.0",
  "description": "react components for budee front-end project",
  "main": "./lib",
  "module": "./es",
  "sideEffects": false,
  "scripts": {
    "start": "webpack-dev-server",
    "bisheng": "bisheng start",
    "site": "bisheng build ",
    "deploy": "npm run site && bisheng gh-pages",
    "clean": "rimraf ./lib ./es",
    "dist": "gulp default",
    "prepare": "npm run build && npm run dist",
    "build": "npm run clean && npm run build:es && npm run build:cjs",
    "build:es": "cross-env BABEL_ENV=es babel ./components --out-dir ./es --copy-files",
    "build:cjs": "cross-env BABEL_ENV=cjs babel ./components --out-dir ./lib --copy-files"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mi-FanFan/b-rc.git"
  },
  "homepage": "https://github.com/Mi-FanFan/b-rc",
  "keywords": [
    "react",
    "component"
  ],
  "author": "Mi-FanFan",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "classnames": "^2.2.5",
    "mff-tabs": "^1.2.4",
    "rc-animate": "^2.4.4",
    "rc-util": "^4.0.2",
    "react-tooltip": "^3.3.0",
    "uuid": "^3.0.1",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "antd": "^3.1.3",
    "autoprefixer": "*",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-transform-require-ignore": "*",
    "bisheng": "0.13.3",
    "bisheng-plugin-antd": "0.3.2",
    "bisheng-plugin-react": "0.2.2",
    "codemirror": "*",
    "create-react-class": "^15.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "*",
    "enzyme": "*",
    "eslint": "*",
    "eslint-config-react-app": "*",
    "eslint-loader": "*",
    "eslint-plugin-flowtype": "*",
