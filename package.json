{
  "name": "b-rc",
  "version": "0.0.5",
  "description": "react components for budee front-end project",
  "main": "./lib",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --progress --colors --port 8081",
    "clean": "rimraf ./lib",
    "build:less": "find lib/ -maxdepth 2 -name 'index.less' -exec lessc --clean-css='--compatibility=ie8 --advanced' {} \\; > ./lib/b-rc.min.css",
    "build": "npm run clean &  babel ./components --out-dir ./lib --copy-files && npm run build:less"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zidaneaa/b-rc.git"
  },
  "homepage": "https://github.com/zidaneaa/b-rc",
  "keywords": [
    "react",
    "component"
  ],
  "author": "freeman007",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "classnames": "^2.2.5",
    "omit.js": "^0.1.0",
    "rc-animate": "^2.3.1",
    "rc-util": "^4.0.2",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "*",
    "babel-cli": "*",
    "babel-core": "*",
    "babel-eslint": "*",
    "babel-loader": "*",
    "babel-plugin-add-module-exports": "*",
    "babel-plugin-istanbul": "*",
    "babel-plugin-transform-class-properties": "*",
    "babel-plugin-transform-require-ignore": "*",
    "babel-plugin-transform-runtime": "*",
    "babel-polyfill": "*",
    "babel-preset-es2015": "*",
    "babel-preset-es2017": "*",
    "babel-preset-react": "*",
    "babel-preset-stage-2": "*",
    "babel-standalone": "*",
    "codemirror": "*",
    "css-loader": "*",
    "enzyme": "*",
    "eslint": "*",
    "eslint-plugin-react": "*",
    "extract-text-webpack-plugin": "*",
    "file-loader": "*",
    "html-webpack-plugin": "*",
    "ignore-styles": "*",
    "json-loader": "*",
    "less": "*",
    "less-loader": "*",
    "less-plugin-clean-css": "*",
    "open-browser-webpack-plugin": "*",
    "postcss-loader": "*",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
